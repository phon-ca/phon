        <xs:annotation>
            <xs:documentation>
                <a href="https://talkbank.org/manuals/CHAT.html#ToneDirection_Marker">CHAT manual
                    section on this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="false">
