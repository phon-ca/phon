        <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
            <xs:documentation>&amp;=; happening, such as sneeze</xs:documentation>
            <xs:documentation>
                <a href="https://talkbank.org/manuals/CHAT.html#Local_Event">CHAT manual section on
                    this topic...</a>
            </xs:documentation>
        </xs:annotation>
    </xs:element>

