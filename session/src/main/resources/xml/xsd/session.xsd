                <a href="https://talkbank.org/manuals/CHAT.html#InterposedWords">CHAT manual section
                    on this topic...</a>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="false">
            <xs:attribute name="who" type="nonemptyStringType" use="required"/>
            <xs:attribute name="said" type="nonemptyStringType" use="required"/>
        </xs:complexType>
    </xs:element>

