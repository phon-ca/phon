                <a href="https://talkbank.org/manuals/CHAT.html#SimpleEvents">CHAT manual section on
                    this topic...</a>
            </xs:documentation>
        </xs:annotation>

        <xs:complexType mixed="false">

            <xs:sequence>
                <xs:choice>
                    <xs:element ref="action"/>
                    <xs:element ref="happening"/>
                    <xs:element ref="otherSpokenEvent"/>
                </xs:choice>

                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="k"/>
                    <xs:element ref="error"/>
                    <xs:element ref="overlap"/>
                    <xs:element ref="ga"/>
                    <xs:element ref="duration"/>
                </xs:choice>
            </xs:sequence>

        </xs:complexType>
    </xs:element>

    <xs:element name="action">
        <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
            <xs:documentation>0</xs:documentation>
            <xs:documentation>
                <a href="https://talkbank.org/manuals/CHAT.html#Action_Code">CHAT manual section on
                    this topic...</a>
            </xs:documentation>
        </xs:annotation>

        <xs:complexType mixed="false"/>
    </xs:element>

